    <title>File Uploads</title>
	<style>
		.sendback{
			background-image: url('/images/fetch.png');
	background-position:center;
	background-repeat:no-repeat;
	background-size:cover;
	height:200px;
	width:200px;
	 margin: 0;
  position: absolute;
  top: 50%;
  left: 50%;
   -ms-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
			margin-top:-3cm;
		}
	  
	  </style>
	
	  </head>
  <body>
   <div class="container" style="margin-top: 3cm;">
<!-- <div class="d-flex justify-content-center">
 <div class="spinner-border" style="width: 6rem; height: 6rem;" role="status">
  <span class="sr-only">Loading...</span>
</div>
</div> -->
	   
	   
	   
	   

	   
	   
	   
	   
	   
	   <div class="ui middle aligned center aligned grid container">
  <div class="ui fluid segment">
     

	 <label for="embedpollfileinput" class="ui huge blue right floated button">
    <i class="ui upload icon"></i> 
   <div class="sendback">
	   	   
    </div>
  </label>
	  </div>
 
	   <form action="/" enctype="multipart/form-data" method="post" class="behind">
          <input type="file" (change)="fileEvent($event)" class="inputfile" id="embedpollfileinput" style="display:none" name="upload" multiple required />
		   <input type="submit" class="btn btn-secondary btn-lg btn-block" name="send" value="send" style="margin-top:7cm">
	  </form>


  </div>
  
</div>
	
		   

	<%= msg%>

    <!-- Optional JavaScript -->
