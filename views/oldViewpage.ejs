        <video src="<%= typeof file != 'undefined' ? file : '' %>" type="video/mp4" autoplay> -->
        <% allfiles.forEach((x)=>{%>
            <ul>
                <a href="\viFw\open - Copy.jpg" download>
                    <%=x%>
                </a>
                <a href="<%=x.slice(7,)%>" download>
                    <%=x.slice(8,)%>
                </a>
                <% })%>

            </ul>
    </div>

